{
  "name": "mobx-react-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for MobX + React project with JSX, ES6 and decorator compilation",
  "scripts": {
    "start": "webpack-dev-server --hot --open",
    "lint": "./node_modules/.bin/eslint src",
    "prettier": "./node_modules/.bin/prettier --write src/**/*.js",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mobxjs/mobx-react-boilerplate.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "mobx",
    "starter-kit"
  ],
  "author": "Michel Weststrate <mweststrate@gmail.com> (http://github.com/mweststrate)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mobxjs/mobx/issues"
  },
  "homepage": "http://mobxjs.github.com/mobx",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.11.1",
    "mobx-react-devtools": "^4.2.11",
    "prettier": "^1.14.3",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.3"
  },
  "dependencies": {
    "dom-to-image": "^2.6.0",
    "dropbox": "^4.0.9",
    "file-saver": "^1.3.8",
    "mobx": "^3.6.2",
    "mobx-react": "^4.4.3",
    "mobx-store": "^3.4.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "save-svg-as-png": "^1.4.6",
    "uglifyjs-webpack-plugin": "^1.3.0"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "useTabs": true,
    "singleQuote": false,
    "semi": false
  }
}
